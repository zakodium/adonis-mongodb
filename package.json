{
  "name": "@zakodium/adonis-mongodb",
  "version": "0.18.1",
  "description": "MongoDB provider for AdonisJS",
  "type": "module",
  "exports": {
    ".": "./build/index.js",
    "./database_provider": "./build/providers/database_provider.js",
    "./odm": "./build/src/odm/index.js"
  },
  "files": [
    "build"
  ],
  "keywords": [
    "adonisjs",
    "adonis",
    "mongo",
    "mongodb",
    "orm",
    "provider"
  ],
  "author": "Zakodium SÃ rl",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "copy:templates": "copyfiles \"stubs/**/*.stub\" build",
    "check-types": "tsc --noEmit",
    "eslint": "eslint . --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run tsc && npm run copy:templates",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "c8 node --import=./tsnode.esm.js --enable-source-maps bin/test.ts",
    "tsc": "npm run clean && tsc --project tsconfig.build.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakodium/adonis-mongodb.git"
  },
  "bugs": {
    "url": "https://github.com/zakodium/adonis-mongodb/issues"
  },
  "homepage": "https://github.com/zakodium/adonis-mongodb#readme",
  "devDependencies": {
    "@adonisjs/auth": "^9.0.1",
    "@adonisjs/core": "^6.2.1",
    "@adonisjs/logger": "^6.0.0",
    "@adonisjs/tsconfig": "^1.2.1",
    "@japa/expect": "^3.0.1",
    "@japa/runner": "^3.1.1",
    "@poppinss/cliui": "^6.3.0",
    "@swc/core": "^1.3.105",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "c8": "^9.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "eslint-config-zakodium": "^8.1.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@poppinss/utils": "^6.7.1",
    "cli-table3": "^0.6.3",
    "lodash": "^4.17.21",
    "mongodb": "^6.3.0"
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  }
}
